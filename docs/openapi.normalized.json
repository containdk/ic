        "/clusters/{clusterId}": {
            "get": {
                "description": "getCluster returns informatin on a specific cluster instance.\n\nA cluster represents a Kubernetes clutser operated and managed with the Netic cloud stack. Information\nis put into the inventory service prior to the first provisioning of the cluster. Some properties\nmay be updated based on metadata from the cluster.\n",
                "operationId": "getCluster",
                "parameters": [
                    {
                        "description": "the cluster id",
                        "name": "clusterId",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "401": {
                        "description": "Missing or invalid credentials (OAuth 2.0 token)",
                        "content": {
                            "application/ld+json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Problem"
                                }
                            },
                            "application/problem+json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Problem"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "The request instance was not found (or user does not have access)",
                        "content": {
                            "application/ld+json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Problem"
                                }
                            },
                            "application/problem+json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Problem"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "",
                        "content": {
                            "application/ld+json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Problem"
                                }
                            },
                            "application/problem+json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Problem"
                                }
                            }
                        }
                    },
