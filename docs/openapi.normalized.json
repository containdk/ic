    "components": {
        "schemas": {
            "Cluster": {
                "description": "Cluster represent a secure cloud stack Kubernetes cluster",
                "type": "object",
                "properties": {
                    "@context": {
                        "description": "Context is defining the JSON-LD context for dereferencing the data as stated in the JSON-LD specification https://www.w3.org/TR/json-ld/#keywords",
                        "type": "object"
                    },
                    "@id": {
                        "description": "ID is identifying the node with an IRI as stated in the JSON-LD specification https://www.w3.org/TR/json-ld/#keywords",
                        "type": "string",
                        "format": "uri"
                    },
                    "@type": {
                        "description": "Type is optional explicit definition of the type of node as stated in the JSON-LD specification https://www.w3.org/TR/json-ld/#keywords",
                        "type": "string"
                    },
                    "apiEndpoint": {
                        "description": "ApiEndpoint is the uri for the Kubernetes api, this may be empty",
                        "type": "string",
                        "format": "uri"
                    },
                    "capacity": {
                        "description": "Capacity is the capacity per node type, i.e., control-plane and worker nodes",
                        "type": "object",
                        "additionalProperties": {
                            "$ref": "#/components/schemas/capacity"
                        }
                    },
                    "clientVersion": {
                        "$ref": "#/components/schemas/version"
                    },
                    "clusterType": {
                        "$ref": "#/components/schemas/ClusterType"
                    },
                    "components": {
                        "description": "Components is a reference to the collection of components detected to be running in the cluster",
                        "type": "string",
                        "format": "uri"
                    },
                    "description": {
                        "description": "Description is the humanreadable cluster description of the cluster",
                        "type": "string"
                    },
                    "environmentName": {
                        "description": "EnvironmentName specifies the name of the environment to which the cluster is associated - this is generic determined by the cluster provider and tenants but suggested to be, e.g., \"production\"",
                        "type": "string"
                    },
                    "fqdn": {
                        "description": "FQDN is deprecated, was a string identifying the cluster with a DNS valid string often also registered in DNS - this will not be provided going forward please use the NRN",
                        "type": "string"
                    },
                    "images": {
                        "description": "Images is a reference to the collection of images discovered from the cluster",
                        "type": "string",
                        "format": "uri"
                    },
                    "infrastructureProvider": {
                        "description": "InfrastructureProvider indicates the provider of the underlying infrastructure, e.g., azure, aws, netic etc. it may not be possible to determine the infrastructure provider",
                        "type": "string"
                    },
                    "kubernetesProvider": {
                        "description": "KubernetesProvider indicates the Kubernetes distribution, e.g., rke2, aks, eks, kubeadm, etc.",
                        "type": "string"
                    },
                    "kubernetesVersion": {
                        "$ref": "#/components/schemas/version"
                    },
                    "name": {
                        "description": "Name uniquely identifying the cluster within the provider of the cluster",
                        "type": "string"
                    },
                    "namespaces": {
                        "description": "Namespaces is a reference to the collection of namespace for the cluster",
                        "type": "string",
                        "format": "uri"
                    },
                    "nrn": {
                        "description": "NRN is the Netic Resource Name uniquely identifying the cluster among all other Netic resource names",
                        "type": "string",
                        "format": "uri"
                    },
                    "provider": {
                        "description": "Provider identification of cluster provider, i.e., the organizational unit responsible for providing the cluster to the tenants",
                        "type": "string",
                        "format": "uri"
                    },
                    "resilienceZone": {
                        "description": "ResilienceZone identified the resilienze to which the cluster is associated",
                        "type": "string",
                        "format": "uri",
                        "example": "https://api.k8s.netic.dk/resilience-zone/internal-1"
                    },
                    "subscription": {
                        "description": "Subscription is the Netic subscription identifier used for accounting - this will only be included when the client is authorized for this information",
                        "type": "string",
                        "format": "uri",
                        "example": "https://api.k8s.netic.dk/subscriptions/123456"
                    },
                    "timestamp": {
                        "description": "Timestamp indicating when data was collected",
                        "type": "string",
                        "format": "date-time"
                    },
                    "vulnerabilities": {
                        "description": "Vulnerabilities is a reference to the collection of vulnerabilities detected from the cluster",
                        "type": "string",
                        "format": "uri"
                    },
                    "workloads": {
                        "description": "Workloads is a reference to the collection of workloads for the cluster",
                        "type": "string",
                        "format": "uri"
                    }
                }
            },
            "ClusterType": {
